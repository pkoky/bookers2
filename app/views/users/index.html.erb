<%= render 'users/list', user: @user, book: @book %>

<h1>Users</h1>
<table>
  <thead>
    <th>image</th>
    <th>name</th>
    <th></th>
  </thead>
  
  <% @users.each do |user| %>
  <tr>
    <td>
      <%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image_square.jpeg" %>
    </td>
      <td><%= user.name %></td>
    <td>
      <%= link_to user_path(user.id) do %>
        Show
      <% end %>
    </td>
  <% end %>

  </tr>
</table>